def sprememba_gbp_v_eur():
    url = 'https://www.google.com/finance/quote/GBP-EUR?' \
          'sa=X&ved=2ahUKEwiVkb_MlrCPAxW79LsIHUoKJfwQmY0JegQICxAu'
    html = requests.get(url).text
    juha = BeautifulSoup(html, 'html.parser')
    sprememba = float(juha.find('div', class_="YMlKec fxKbKc").text)
    return sprememba


sprememba = sprememba_gbp_v_eur()


def gbp_v_eur(gbp):
    gbp = float(gbp.strip('£'))
    eur = round(gbp * sprememba, 2)
    return f'{eur:.2f}€'